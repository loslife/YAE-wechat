<% if(env === "dev"){ %>

<%- Loader()
        .js('/m-festival/static/done.js')
        .css("/m-festival/static/css/done.css")
        .done(config) %>

<% }else{ %>

<%- Loader()
        .js('/m-festival/done.min.js')
        .css("/m-festival/css/done.min.css")
        .done(config) %>

<% } %>

<div id="m-festival-done">

    <div id="wrapper">

        <img src="../../../resource/present_got_success.jpg">

        <div id="message">
            <img src="../../../resource/duigou.png">
            <span id="msg_success">领取成功！</span>
        </div>
    </div>

</div>

